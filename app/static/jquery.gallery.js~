var ImageData = Class.extend({
    init: function(title, medium, height, width){
        this.title = title;
        this.medium = medium;
        this.height = height;
        this.width = width;
    },
    getTitle: function(){
        return this.title;
    },
    getMedium: function(){
        return this.medium;
    },
    getHeight: function(){
        return this.height;
    },
    getWidth: function(){
        return this.width;
    }
});
